int countPrimes(int n) {
        vector<int>dp(n,1);

        for(int i=2;i*i<n;i++){
            if(dp[i]){
            for(int j=i*i;j<n;j+=i){
               dp[j]=0;
            }
            }
        }

        int ans=0;
        for(int i=2;i<n;i++){
            if(dp[i]){
                ans++;
            }
        }

        return ans;
    }
